{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "no-underscore-dangle": ["error", { "allow": ["_id", "__v"], "allowAfterThis": true, "allowAfterSuper": true, "allowAfterThisConstructor": true }],
    "no-param-reassign": ["error", {
        "props": false
    }],
    "no-console": "off"
        
  }
}